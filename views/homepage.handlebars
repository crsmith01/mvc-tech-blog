<!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<div class="row mb-4 post">
  <div class="col-md-5">
    <p>Posted by {{post.user_id}} on {{format_date post.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{post.text}}
    </p>
  </div>
</div>
{{/each}}
